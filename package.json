{
  "type": "commonjs",
  "name": "lscripts",
  "version": "1.0.0",
  "description": "lscripts is a command tool that allows you to list and choose a script from your package.json easely.",
  "main": "./dist/main.js",
  "exports": {
    "require": "./dist/main.js"
  },
  "bin": {
    "lscripts": "./dist/main.js"
  },
  "files": [
    "dist/*",
    "package.json"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky"
  },
  "author": "@julesfrn",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/julesfrn/lscripts.git"
  },
  "keywords": [
    "lscripts",
    "cli"
  ],
  "dependencies": {
    "@inquirer/search": "^3.1.1",
    "clipboardy": "^4.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "husky": "^9.1.7",
    "semantic-release": "^24.2.7",
    "typescript": "^5.9.2"
  }
}
